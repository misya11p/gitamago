{
  "name": "gitamago",
  "version": "1.0.0",
  "manifest_version": 3,
  "description": "description",
  "action": {
    "default_popup": "popup.html"
  },
  "options_page": "options.html",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "host_permissions": ["*://*/*"]
}
