{
  "name": "GiTamago",
  "version": "1.0.0",
  "manifest_version": 3,
  "description": "Monitor recent your github activity in 'Piyo'.",
  "action": {
    "default_popup": "src/popup.html"
  },
  "options_page": "src/options.html",
  "permissions": ["storage"],
  "icons": {
    "16": "imgs/icons/icon16.png",
    "48": "imgs/icons/icon48.png",
    "128": "imgs/icons/icon128.png"
  },
  "host_permissions": ["https://gsbmtic6zb.execute-api.ap-northeast-1.amazonaws.com/app"]
}
